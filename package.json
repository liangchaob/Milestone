  "dependencies": {
    "nanoid": "^5.0.6",
    "sql.js": "^1.10.2",
    "express": "^4.21.0",
    "cors": "^2.8.5",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "vite": "^5.4.10"
  }
