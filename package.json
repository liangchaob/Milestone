{
  "name": "milestone-v3",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:client": "vite --config v3/vite.config.js",
    "dev:server": "nodemon -e js,json -w server server/index.js",
    "dev": "concurrently -n server,client -c green,blue \"npm:dev:server\" \"npm:dev:client\"",
